{
  "description": "Test custom big model mapping via env variables",
  
  // 覆盖环境变量来测试自定义大模型映射
  "env": {
    "CLAUDE_PROXY_BIG_MODEL": "custom-gpt-4"
  },
  
  "claude_request": {
    "model": "claude-3-opus-20240229",
    "max_tokens": 100,
    "messages": [
      {
        "role": "user", 
        "content": "Test custom big model mapping"
      }
    ]
  },
  
  "expected_openai_request": {
    "model": "custom-gpt-4", // opus -> custom big model
    "messages": [
      {
        "role": "user",
        "content": "Test custom big model mapping"
      }
    ],
    "max_tokens": 100,
    "stream": false
  }
}